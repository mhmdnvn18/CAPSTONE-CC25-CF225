{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "public": "public",
  "routes": [
    { "src": "/sw.js", "dest": "/sw.js" },
    { "src": "/manifest.json", "dest": "/manifest.json" },
    { "src": "/model/(.*)", "dest": "/model/$1" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/result.html", "dest": "/result.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
